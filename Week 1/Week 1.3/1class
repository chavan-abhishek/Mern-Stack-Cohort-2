class Animal {
    constructor(name, legCount, speaks) {
        this.name = name;
        this.legCount = legCount;
        this.speaks = speaks;
    }

    static myType() {
        console.log(" abhishek is a animal" + this.legCount);
    }

    speak() {
        console.log("hii there i am ", this.name);
    }
}

let dog = new Animal("dog", 4, "bhow bhow");  // create object
let cat = new Animal("cat", 4, "meow");

dog.speak();  // call function on an object
Animal.myType();

